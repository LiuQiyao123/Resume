# 服务器配置
PORT=3000
NODE_ENV=development

# MongoDB配置
MONGODB_URI=mongodb://localhost:27017/resume_db

# JWT配置
JWT_SECRET=your_jwt_secret_key
JWT_EXPIRES_IN=7d

# 文件上传配置
UPLOAD_DIR=uploads
MAX_FILE_SIZE=5242880 # 5MB

# 微信小程序配置
WECHAT_APPID=your_appid
WECHAT_SECRET=your_secret

# 日志配置
LOG_LEVEL=debug
LOG_DIR=logs

# 缓存配置
CACHE_TTL=3600 # 1小时 