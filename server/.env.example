# 服务器配置
PORT=3000
NODE_ENV=development

# 数据库配置
MONGODB_URI=mongodb://localhost:27017/ai_resume

# JWT配置
JWT_SECRET=your_jwt_secret_key
JWT_EXPIRES_IN=7d
JWT_REFRESH_SECRET=your_refresh_secret_key
JWT_REFRESH_EXPIRES_IN=30d

# 微信小程序配置
WX_APP_ID=your_wx_app_id
WX_APP_SECRET=your_wx_app_secret

# 微信支付配置
WX_MCH_ID=your_mch_id
WX_PAY_API_KEY=your_pay_api_key
WX_PAY_NOTIFY_URL=https://your-domain.com/api/payment/notify

# DeepSeek AI API配置
DEEPSEEK_API_KEY=your_deepseek_api_key
DEEPSEEK_API_URL=https://api.deepseek.com/v1

# 对象存储配置
COS_SECRET_ID=your_cos_secret_id
COS_SECRET_KEY=your_cos_secret_key
COS_REGION=ap-beijing
COS_BUCKET=your-bucket-name

# 日志配置
LOG_LEVEL=info
LOG_DIR=logs 